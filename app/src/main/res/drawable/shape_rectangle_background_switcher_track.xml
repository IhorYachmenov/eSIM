<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/background_switcher_blue" />
            <corners android:radius="@dimen/radius_switcher_track"/>

            <stroke android:width="@dimen/size_width_stroke_switcher_btn" android:color="@color/background_switcher_blue" />
        </shape>
    </item>

    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/background_switcher_blue" />
            <corners android:radius="@dimen/radius_switcher_track"/>

            <stroke android:width="@dimen/size_width_stroke_switcher_btn" android:color="@color/background_switcher_blue" />
        </shape>
    </item>
</selector>